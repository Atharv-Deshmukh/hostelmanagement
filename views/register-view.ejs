<h1 class="text-center">Pending Registration</h1>

<% for(var i=0; i<student.length; i++){ %>
<div class="card container">
    <form action="/admin/register/add" method="post">
    <input class="sr-only" type="text" name="_id" value="<%= student[i]._id %>" id="">
    <div class="card-header">
        <%= student[i].name %>
    </div>
    <div class="card-body">
      <h5 class="card-title"><%= student[i].email %> | <%= student[i].number %></h5>
      <p class="card-text">Department: <%= student[i].dept %> | Roll No.: <%= student[i].roll_no %></p>
      <p class="card-text">Address: <%= student[i].add %></p>
      <p class="card-text">
        Enter Room No to Allocate: 
        <select name="room" id="" required>
            <option value="room" disabled selected>Select room to allocate</option>
            <% for(var j=0; j<rm.length; j++){ %>
                <option value="<%= rm[j].room_no %>">Room No: <%= rm[j].room_no %> - Occupied: <%= rm[j].status %></option>
            <% } %>
        </select>
      </p>
      <input type="submit" class="btn btn-primary" name="" id="">
    </div>
    </form>
  </div>
 <br><br>
<% } %>

